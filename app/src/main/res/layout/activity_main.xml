<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:layout_margin="8dp">

    <TextView
        android:id="@+id/tahajjud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tahajjud"
        android:textSize="18sp"
        />
<TextView
        android:id="@+id/witir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/witir"
    android:textSize="18sp"/>
<TextView
        android:id="@+id/rawatib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/rawatib"/>
    <TextView
        android:id="@+id/duha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dhuha"/>
<TextView
    android:textSize="18sp"
        android:id="@+id/istikoroh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/istikoroh"/>
<TextView
    android:textSize="18sp"
        android:id="@+id/tahiyatullmasjid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tahiyatullmasjid"/>
<TextView
    android:textSize="18sp"
        android:id="@+id/shalat_Suruk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/syuruq"/>





</LinearLayout>